%div.row.well
  %div.col-md-2
  %div.col-md-6

    %div{:class => "panel panel-yellow"}
      %div.panel-heading
        %div.row
          %div.col-xs-9.text-left
            = "Details about #{@data_result.band_name} band"
      %div.panel-footer
        %span.pull-left
          %table.table.table-striped.table-bordered
            %tr
              %td
                Tour Information
                %ul.list-unstyled #{@data_result.tour_name}
                %br

                %br Years Active


                %br People that played with #{@data_result.band_name}

                %br Associated bands that members of Kraftwerk have played with #{@data_result.band_name}

        %div.clearfix


